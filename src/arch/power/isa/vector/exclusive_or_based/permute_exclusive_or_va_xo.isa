format IntArithOp {
    45: vpermxor({{
        uint8_t indexA;
        uint8_t indexB;
        uint8_t src1;
        uint8_t src2;
        for(int i=0; i < 16; i++) {
            indexA = bits(Vc_ub[i], 7, 4);
            indexB = bits(Vc_ub[i], 3, 0);
            src1 = Va_ub[15-indexA];
            src2 = Vb_ub[15-indexB];
            Vt_ub[i] = src1 ^ src2;
        }
    }});
}